vel = deepExtend({}, object);\r\n    keys.forEach((k) => {\r\n        if (level && typeof level[k] !== 'undefined') {\r\n            level = level[k];\r\n        }\r\n    });\r\n    return typeof level === 'undefined' ? defaultValue : level;\r\n}\r\nexport function getPageForRowIndex(index, perPage) {\r\n    // we need to add 1 to convert 0-based index to 1-based page number.\r\n    return Math.floor(index / perPage) + 1;\r\n}\r\n//# sourceMappingURL=data:application/json;base64,","originalSourcemap":{"version":3,"file":"helpers.js","sourceRoot":"","sources":["../../../../../projects/ng2-smart-table/src/lib/lib/helpers.ts"],"names":[],"mappings":[[[0,0,0,0],[7,0,0,7],[9,0,0,9],[18,0,0,18],[20,0,0,20],[26,0,0,26],[34,0,0,34],[35,0,0,35]],[[0,0,2,0]],[],[],[],[],[],[],[],[[3,0,10,3]],[[0,0,11,0],[6,0,11,6],[7,0,11,7],[13,0,11,13],[23,0,11,23],[26,0,11,26],[36,0,11,35],[39,0,11,38],[46,0,11,57]],[[4,0,12,2],[8,0,12,6],[17,0,12,15],[18,0,12,16],[24,0,12,22],[27,0,12,25],[28,0,12,26],[32,0,12,30],[39,0,12,37],[48,0,12,46],[49,0,12,47],[50,0,12,48],[51,0,12,49],[56,0,12,54],[64,0,12,62],[66,0,12,64]],[[8,0,13,4],[15,0,13,11],[20,0,13,16],[21,0,13,17]],[[5,0,14,3]],[[4,0,16,2],[8,0,16,6],[17,0,16,15],[18,0,16,16],[24,0,16,22],[27,0,16,25],[28,0,16,26],[30,0,16,28]],[[8,0,17,4],[15,0,17,11],[24,0,17,20],[25,0,17,21],[26,0,17,22],[27,0,17,23],[28,0,17,24]],[[5,0,18,3]],[[4,0,20,2],[10,0,20,8],[16,0,20,14],[19,0,20,17],[28,0,20,26],[29,0,20,27],[30,0,20,28],[31,0,20,29],[32,0,20,30]],[[4,0,22,2],[59,0,22,57]],[[4,0,23,2],[10,0,23,8],[14,0,23,12],[17,0,23,15],[22,0,23,20],[23,0,23,21],[32,0,23,30],[33,0,23,31],[38,0,23,36],[39,0,23,37],[43,0,23,41],[44,0,23,42],[53,0,23,51],[55,0,23,53],[56,0,23,54],[57,0,23,55],[58,0,23,56]],[[4,0,25,2],[8,0,25,6],[11,0,25,9],[13,0,25,11],[16,0,25,14],[17,0,25,15]],[[4,0,27,2],[8,0,27,6],[9,0,27,7],[16,0,27,14],[17,0,27,15],[18,0,27,16],[21,0,27,24],[23,0,27,26],[25,0,27,28]],[[8,0,28,4],[55,0,28,51]],[[8,0,29,4],[12,0,29,8],[19,0,29,15],[22,